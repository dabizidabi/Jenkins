#!groovy
// Check ubuntu properties
properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'master'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timeout()
    }
    stages {
        stage("First step") {
            steps {
                sh 'ssh root@192.168.1.102 \'hostname\''
            }
        }
        stage("Second step") {
            steps {
                sh 'ssh root@192.168.1.102 \'uptime\''
            }
        }
    }
}
